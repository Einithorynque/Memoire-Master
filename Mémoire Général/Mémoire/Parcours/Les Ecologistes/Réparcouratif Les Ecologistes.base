views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("Parcours/Les Ecologistes")
        - Type == ["PARCOURS"]
    order:
      - file.name
      - Parcours
      - Récurrence
      - Description
      - Réponse au problème
    sort:
      - property: Réponse au problème
        direction: DESC
      - property: Parcours
        direction: ASC
